{
    "name": "saddamsaifi1992/admin-panel-bootstrap",
    "description": "admin panel",
    "keywords": [
        "saddamsaifi1992",
        "admin-panel-bootstrap"
    ],
    "homepage": "https://github.com/saddamsaifi1992/admin-panel-bootstrap",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saddam Hussain",
            "email": "saddam1234321@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Saddamsaifi1992\\AdminPanelBootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saddamsaifi1992\\AdminPanelBootstrap\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saddamsaifi1992\\AdminPanelBootstrap\\AdminPanelBootstrapServiceProvider"
            ],
            "aliases": {
                "AdminPanelBootstrap": "Saddamsaifi1992\\AdminPanelBootstrap\\AdminPanelBootstrapFacade"
            }
        }
    }
}
